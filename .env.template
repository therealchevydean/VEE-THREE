# VEE-THREE Environment Configuration Template
# Copy this file to .env and fill in the values.

# --- AI Services ---
# Gemini API Key for text, image, and video generation
GEMINI_API_KEY=your_gemini_api_key_here

# --- Authentication & Integrations ---
# Google OAuth (for Calendar, Drive, etc.)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# GitHub OAuth (for repo management, commits)
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Vercel (for deployments)
VERCEL_TOKEN=your_vercel_token

# eBay API (for e-commerce features)
EBAY_APP_ID=your_ebay_app_id
EBAY_CERT_ID=your_ebay_cert_id
EBAY_DEV_ID=your_ebay_dev_id

# Social Media (Future Phase)
# X_API_KEY=
# INSTAGRAM_TOKEN=

# --- Backend Configuration ---
# Port for the backend server
PORT=3001
# Database Connection String (Future Phase)
# DATABASE_URL=postgresql://user:password@localhost:5432/vee_db
